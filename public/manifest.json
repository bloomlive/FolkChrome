{
    "name": "ViljandiFolker",
    "author": "Daniel Sepp",
    "description": "Unofficial Viljandi Folk schedule extension that allows you to easily create Google Calendar events from the schedule.",
    "version": "1.0.1",
    "manifest_version": 3,
    "icons": {
        "16": "icons/16.png",
        "32": "icons/32.png",
        "48": "icons/48.png",
        "128": "icons/128.png",
        "192": "icons/192.png",
        "384": "icons/384.png"
    },
    "content_scripts": [
        {
            "js": ["background.js"],
            "matches": [
                "https://www.viljandifolk.ee/*",
                "https://viljandifolk.ee/*"
            ]
        }
    ]
}
